{
    "extends": "standard",
    "env": {
        "browser": false,
        "commonjs": true
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off",
        "comma-dangle": "off",
        "no-multiple-empty-lines": "warn"
    },
    "globals": {}
}